{
  "name": "Mavii Search",
  "version": "1.0",
  "description": "Find the best stuff with Mavii.",
  "manifest_version": 3,
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "pika",
      "keyword": "pika",
      "search_url": "https://mavii.com/search?q={searchTerms}",
      "favicon_url": "https://icon.horse/icon/startpage.com",
      "suggest_url": "https://mavii.com/api/suggestions?q={searchTerms}&format=chrome",
      "encoding": "UTF-8",
      "is_default": true
    }
  }
}
